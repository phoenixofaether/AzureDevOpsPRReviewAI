{
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "https://applog.ost.ch:5341"
        }
      }
    ]
  },
  "AzureDevOps": {
    "BaseUrl": "https://dev.azure.com/{your-production-organization}"
  },
  "Redis": {
    "ConnectionString": "{production-redis-connection-string}"
  },
  "Repository": {
    "LocalStoragePath": "D:/repos",
    "CleanupIntervalHours": 12,
    "MaxDiskUsageGB": 50
  },
  "Webhook": {
    "BasicAuth": {
      "Username": "webhook-prod-user",
      "Password": "use-strong-password-here"
    }
  }
}