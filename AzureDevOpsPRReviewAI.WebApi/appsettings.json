{
  "Serilog": {
    "Using": ["Serilog.Sinks.Seq"],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": ["FromLogContext"],
    "WriteTo": [
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341"
        }
      }
    ]
  },
  "AzureDevOps": {
    "BaseUrl": "https://dev.azure.com/davidoberkalmsteiner"
  },
  "Claude": {
    "Model": "claude-3-haiku-20240307", // later use at least 3.7, 4.1 and 4 cost the same
    "MaxTokens": 200000
  },
  "Repository": {
    "LocalStoragePath": "./repos",
    "CleanupIntervalHours": 24,
    "MaxDiskUsageGB": 10
  },
  "Webhook": {
    "BasicAuth": {
      "Username": "doberkalmsteinerProject",
      "Password": "change-this-in-production"
    }
  },
  "VectorDatabase": {
    "Provider": "Qdrant",
    "ConnectionString": "http://localhost:6333",
    "CollectionName": "code-embeddings",
    "VectorSize": 768,
    "Distance": "Cosine"
  },
  "NomicEmbedding": {
    "ApiUrl": "http://localhost:8000",
    "Model": "nomic-ai/nomic-embed-code",
    "BatchSize": 32,
    "MaxTokens": 8192
  },
  "Embeddings": {
    "Provider": "Nomic",
    "Model": "nomic-ai/nomic-embed-code",
    "BatchSize": 32,
    "MaxTokensPerChunk": 8192
  },
  "RepositoryConfiguration": {
    "StoragePath": "./config"
  },
  "UserManagement": {
    "UserStoragePath": "./users",
    "ActivityStoragePath": "./activity"
  },
  "AllowedHosts": "*"
}
